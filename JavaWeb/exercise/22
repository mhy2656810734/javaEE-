        $.ajax({
            type:'get',
            url:'https://www.sogou.com?studentName=xiaoma',
            success:function(){
                console.log('当服务器的响应到达浏览器后,浏览器触发该回调,通知到代码中');
            }
        });
        console.log('代码继续往下执行');
    </script>
